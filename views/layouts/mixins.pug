mixin form-name-contacts(userSurname,userName,userLastname,userPhone,userEmail)
	ul.form-name-contacts__list-name
		li.form-name-contacts__input--error
			label(for=userSurname) Фамилия
			input(type="text" id=userSurname autocomplete="off").dflt-input
		li 
			label(for=userName) Имя
			input(type="text" id=userName autocomplete="off").dflt-input
		li 
			label(for=userLastname) Отчество
			input(type="text" id=userLastname autocomplete="off").dflt-input
	ul.form-name-contacts__list-contacts
		li.form-name-contacts__input--error
			label(for=userPhone) Телефон
			input(type="text" id=userPhone placeholder="+7" autocomplete="off").dflt-input
		li 
			label(for=userEmail) E-mail
			input(type="text" id=userEmail autocomplete="off").dflt-input

